{
  "name": "dgz-ui",
  "version": "1.0.19",
  "type": "module",
  "files": [
    "dist"
  ],
  "author": "alisher1119",
  "keywords": [
    "shadcn",
    "styled-components",
    "tailwindcss@4",
    "dgz-ui",
    "ui-lib",
    "react.js",
    "typescript"
  ],
  "description": "Custom ui library using React.js, Shadcn, TailwindCSS, Typescript",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Alisher1119/dgz-ui.git"
  },
  "main": "./dist/dgz-ui.umd.js",
  "module": "./dist/dgz-ui.es.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/dgz-ui.es.js",
      "require": "./dist/dgz-ui.umd.js"
    },
    "./dist/dgz-ui.css": "./dist/dgz-ui.css"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build && tsc -b",
    "test": "vitest run",
    "preview": "vite preview",
    "format": "prettier . --write",
    "format:check": "prettier . --check",
    "lint": "eslint ./src --fix --max-warnings 50",
    "typecheck": "tsc --noEmit",
    "publish:major": "npm version major && npm publish",
    "publish:minor": "npm version minor && npm publish",
    "publish:patch": "npm version patch && npm publish",
    "prepare": "husky"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.0.1",
    "@radix-ui/react-accordion": "^1.2.11",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.2",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-popover": "^1.1.14",
    "@radix-ui/react-scroll-area": "^1.2.9",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.5",
    "@radix-ui/react-tabs": "^1.1.12",
    "@radix-ui/react-tooltip": "^1.2.7",
    "@remixicon/react": "^4.6.0",
    "@tailwindcss/vite": "^4.1.7",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "date-fns": "^4.1.0",
    "filepond-plugin-file-validate-size": "^2.2.8",
    "filepond-plugin-file-validate-type": "^1.2.9",
    "filepond-plugin-image-preview": "^4.6.12",
    "filepond-plugin-image-resize": "^2.0.10",
    "lodash": "^4.17.21",
    "lucide-react": "^0.511.0",
    "next-themes": "^0.4.6",
    "react-day-picker": "^9.7.0",
    "react-filepond": "^7.1.3",
    "react-hook-form": "^7.56.4",
    "react-select": "^5.10.1",
    "sonner": "^2.0.3",
    "styled-components": "^6.1.18",
    "tailwind-merge": "^3.3.0",
    "tailwindcss": "^4.1.7",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.25.30"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.0",
    "@rollup/plugin-typescript": "^12.1.2",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/lodash": "^4.17.17",
    "@types/node": "^22.15.21",
    "@types/react": "^19.1.6",
    "@types/react-dom": "^19.1.5",
    "@vitejs/plugin-react": "^4.4.1",
    "eslint": "^9.25.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jsdom": "^26.1.0",
    "prettier": "^3.5.3",
    "rollup-plugin-dts": "^6.2.1",
    "tslib": "^2.8.1",
    "tw-animate-css": "^1.3.0",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.30.1",
    "vite": "^6.3.5",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^3.1.4"
  },
  "peerDependencies": {
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  }
}
